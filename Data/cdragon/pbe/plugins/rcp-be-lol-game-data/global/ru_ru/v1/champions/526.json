    "spells": [
        {
            "spellKey": "q",
            "name": "Пробивающий удар",
            "abilityIconPath": "/lol-game-data/assets/ASSETS/Characters/Rell/HUD/Icons2D/RellQ.png",
            "abilityVideoPath": "champion-abilities/0526/ability_0526_Q1.webm",
