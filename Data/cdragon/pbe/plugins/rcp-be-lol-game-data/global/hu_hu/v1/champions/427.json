    "passive": {
        "name": "Az erdő barátja",
        "abilityIconPath": "/lol-game-data/assets/ASSETS/Characters/Ivern/HUD/Icons2D/IvernP.png",
        "abilityVideoPath": "champion-abilities/0427/ability_0427_P1.webm",
        "abilityVideoImagePath": "champion-abilities/0427/ability_0427_P1.jpg",
        "description": "A nem hatalmas szörnyeket Ivern nem támadhatja meg, illetve azok sem támadhatják meg őt. Ivern mágikus lugasokat ültethet a dzsungeltáborokba. Ezek idővel megnőnek. Amikor a lugas teljesen megnő, Ivern felszabadíthatja a szörnyeket, jutalmul pedig aranyat és tapasztalatot kap."
    },
    "spells": [
        {
            "spellKey": "q",
            "name": "Gyökércsapda",
            "abilityIconPath": "/lol-game-data/assets/ASSETS/Characters/Ivern/HUD/Icons2D/IvernQ.png",
            "abilityVideoPath": "champion-abilities/0427/ability_0427_Q1.webm",
            "abilityVideoImagePath": "champion-abilities/0427/ability_0427_Q1.jpg",
            "cost": "@Cost@ @AbilityResourceName@",
