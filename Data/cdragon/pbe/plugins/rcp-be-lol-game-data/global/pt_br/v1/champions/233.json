        {
            "spellKey": "e",
            "name": "Grito Arrepiante",
            "abilityIconPath": "/lol-game-data/assets/ASSETS/Characters/Briar/HUD/Icons2D/BriarE.png",
            "abilityVideoPath": "champion-abilities/0233/ability_0233_E1.webm",
