    "spells": [
        {
            "spellKey": "q",
            "name": "Plasmaspaltung",
            "abilityIconPath": "/lol-game-data/assets/ASSETS/Characters/Velkoz/HUD/Icons2D/VelKoz_Q1.png",
            "abilityVideoPath": "champion-abilities/0161/ability_0161_Q1.webm",
            "abilityVideoImagePath": "champion-abilities/0161/ability_0161_Q1.jpg",
            "cost": "@Cost@ @AbilityResourceName@",
            "cooldown": "@Cooldown@&nbsp;s %i:cooldown%",
            "description": "Vel'Koz feuert ein Plasmageschoss ab, das sich bei erneuter Aktivierung oder bei Kontakt mit einem Gegner in zwei Teile aufspaltet. Das Geschoss verlangsamt und verursacht bei einem Treffer Schaden.",
