    "passive": {
        "name": "Είμαστε περισσότεροι",
        "abilityIconPath": "/lol-game-data/assets/ASSETS/Characters/Naafiri/HUD/Icons2D/Icons_Naafiri_P.png",
        "abilityVideoPath": "champion-abilities/0950/ability_0950_P1.webm",
