    "passive": {
        "name": "Sárkányedzés",
        "abilityIconPath": "/lol-game-data/assets/ASSETS/Characters/Smolder/HUD/Icons2D/Icons_Smolder_Passive.png",
        "abilityVideoPath": "champion-abilities/0901/ability_0901_P1.webm",
        "abilityVideoImagePath": "champion-abilities/0901/ability_0901_P1.jpg",
        "description": "Ha a képességeivel hősöket talál el, vagy a Szupertüzes lehelet képességgel ellenfeleket öl meg, Sárkányedzés jelzőt kap. A jelzők növelik Smolder alapképességeinek sebzését."
    },
    "spells": [
        {
            "spellKey": "q",
            "name": "Szupertüzes lehelet",
            "abilityIconPath": "/lol-game-data/assets/ASSETS/Characters/Smolder/HUD/Icons2D/Icons_Smolder_Q1.png",
            "abilityVideoPath": "champion-abilities/0901/ability_0901_Q1.webm",
